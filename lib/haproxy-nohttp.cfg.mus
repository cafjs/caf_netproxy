global
	log 127.0.0.1	local0
	log 127.0.0.1	local1 notice
	maxconn 10000
	user haproxy
	group haproxy
	daemon

defaults
	log	global
	mode	http
	option	dontlognull
	retries	3
	option redispatch
	timeout connect	5000
	timeout client	50000
	timeout server	50000
  	timeout tunnel        3600s


{{#ports}}
listen	{{{name}}} 0.0.0.0:{{{localPort}}}
	mode	tcp
	server	srv_{{{name}}} {{{remoteHost}}}:{{{remotePort}}}

{{/ports}}
